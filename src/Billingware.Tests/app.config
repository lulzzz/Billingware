    <!--Serilog settings-->
    <add key="serilog:minimum-level" value="Info" />
    <add key="serilog:using:RollingFile" value="Serilog.Sinks.RollingFile" />
    <add key="serilog:write-to:RollingFile.pathFormat" value="logs\log-{Date}.txt" />
